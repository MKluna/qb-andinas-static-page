---
import Layout from "../layouts/Layout.astro";
import Hero from "../components/sections/Hero.astro";
import CTA from "../components/sections/CTA.astro";
import Brands from "../components/sections/Brands.astro";
import Contact from "../components/sections/Contact.astro";
import ServicesSection from "../components/sections/principal_sections/ServicesSection.astro";
import ConsultingSection from "../components/sections/principal_sections/ConsultingSection.astro";
import ExperienceSection from "../components/sections/principal_sections/ExperienceSection.astro";
---

<Layout title="Empresa">
  <main class="flex flex-col gap-y-20 md:gap-y-32 overflow-hidden">
    <Hero />
    <CTA />
    <Brands />
    <ServicesSection/>
    <ConsultingSection/>
    <ExperienceSection/>
    <Contact />
